<?xml version="1.0" encoding="utf-8"?>
<Defs>
  
  <!--不填的字段会按照原版的生成逻辑-->
  <!--Params that didn't filled will generate as the orignal logic-->
  
  <FixedPawnGenerate.FixedPawnDef>
    <defName>TestPawn1</defName>

    <generateWeight>1</generateWeight>
    <generateRate>0.0</generateRate><!--0 - 1-->
    <isUnique>true</isUnique>

    <faction>PlayerColony</faction>
    <race>Human</race>
    <pawnKind>Colonist</pawnKind>

    <age>13.5</age>
    <chronologicalAge>200</chronologicalAge>
    <!--1 for Male, 2 for Female-->
    <!--1 是男，2是女-->
    <gender>2</gender>
    
    <firstName>TestFirstName</firstName>
    <nickName>TestNickName</nickName>
    <lastName>TestLastName</lastName>

    <beard>Anchor</beard>
    <hair>Mop</hair>
    <hairColor>(237, 202, 156)</hairColor> <!--R,G,B-->

    <headType>Female_AverageWide</headType>
    <faceTatoo MayRequire="ludeon.rimworld.royalty">Face_Pantheon</faceTatoo>

    <bodyType>Male</bodyType>
    <bodyTatoo MayRequire="ludeon.rimworld.royalty">Body_Tiger</bodyTatoo>
    <skinColor>(155,155,155)</skinColor>

    <childHood>FallenProdigy40</childHood>
    <adultHood>Archaeologist85</adultHood>

    <favoriteColor>(237, 202, 156)</favoriteColor>

    <equipment>
      <li>Gun_BoltActionRifle</li>
    </equipment>

    <inventory>
      <li>MealSurvivalPack</li>
    </inventory>

    <apparel>
      <li>Apparel_Pants</li>
      <li>Apparel_BasicShirt</li>
    </apparel>

    <skills>
      <li>
        <skill>Shooting</skill>
        <level>10</level>
        <passion>0</passion>
      </li>
      <li>
        <skill>Cooking</skill>
        <level>5</level>
        <passion>1</passion>
      </li>
    </skills>

    <hediffs>
      <li>
        <hediff>Gunshot</hediff>
        <severity>0.5</severity>
        <bodyPart>Head</bodyPart>
      </li>
    </hediffs>

    <traits>
      <li>Kind</li>
      <li>Psychopath</li>
    </traits>
  </FixedPawnGenerate.FixedPawnDef>

